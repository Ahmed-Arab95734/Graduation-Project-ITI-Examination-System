	measure 'Overall Failure Rate' =
			
			DIVIDE(
			    [Failure],
			    [Total Students],
			    0
			)
		formatString: 0.00%;-0.00%;0.00%
		lineageTag: 35309638-cfe8-4d5e-99f1-b44c1604f713

	measure 'Track Failure Rate' = ```
			
			VAR StudentsInTrack = 
			    CALCULATE(
			        COUNTROWS(DimStudent),
			        ALLEXCEPT(DimTrack, DimTrack[Track_Name])
			    )
			VAR FailuresInTrack = 
			    CALCULATE(
			        [Failure],
			        ALLEXCEPT(DimTrack, DimTrack[Track_Name])
			    )
			RETURN
			DIVIDE(FailuresInTrack, StudentsInTrack, 0)
			
			```
		formatString: 0.00%;-0.00%;0.00%
		lineageTag: ada080ed-dabb-4e5b-bbac-b6c9eb25c7a5

