			)
		formatString: 0
		lineageTag: 5d9accfc-a1a2-4a37-9d14-4969dc411a6e

	measure 'Avg. Course Grade' =
			
			CALCULATE(
			    AVERAGE(FactStudentPerformance[Student_Grade]),
			    ALLEXCEPT(DimCourse, DimCourse[CourseKey])
			)
		lineageTag: 546ea337-e0b6-4594-be6c-b4ff0bdcb73f

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure 'Hardest Course1' =
			
			VAR CourseGrades =
			    SUMMARIZE(
			        DimCourse,
			        DimCourse[Course_Name],
			        "AvgGrade", [Avg. Course Grade]
			    )
			VAR MinGrade = MINX(CourseGrades, [AvgGrade])
			RETURN
			    MAXX(
			        FILTER(CourseGrades, [AvgGrade] = MinGrade),
			        DimCourse[Course_Name]
			    )
		lineageTag: 1747152d-1055-4f36-8599-7c258b25d1a1

