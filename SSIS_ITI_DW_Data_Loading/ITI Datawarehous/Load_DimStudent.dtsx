                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">/*
This query loads data for the DimStudent table. It performs three key transformations:
1. Denormalizes Track, Branch, and Intake information via the [Group] table.
2. Pivots the Student_Social table to turn rows into columns.
3. Selects only one phone number from the Student_Phone table.
*/

-- CTE 1: Handle multiple phone numbers.
WITH PhoneCTE AS (
    SELECT 
        Student_ID, 
        Phone,
        ROW_NUMBER() OVER(PARTITION BY Student_ID ORDER BY Phone) as rn
    FROM 
        Student_Phone
),

-- CTE 2: Handle social media links using the robust PIVOT syntax.
SocialCTE AS (
    SELECT Student_ID, [Facebook], [LinkedIn], [GitHub], [X], [Instagram]
    FROM (
        -- This subquery selects the raw data to be pivoted
        SELECT Student_ID, Social_Type, Social_Url
        FROM Student_Social
    ) AS SourceTable
    PIVOT (
        MAX(Social_Url)
        FOR Social_Type IN ([Facebook], [LinkedIn], [GitHub], [X], [Instagram])
    ) AS PivotTable
)

-- Main query to select all data and join the CTEs
SELECT 
    s.Student_ID,
    s.Student_Fname + N' ' + s.Student_Lname AS Student_FullName,
    s.Student_Gender,
    s.Student_Mail,
    s.Student_Address,
    s.Student_Birthdate,
    p.Phone,  -- From PhoneCTE
    s.Student_Marital_Status,
    s.Student_Faculty,
    s.Student_Faculty_Grade,
    s.Student_ITI_Status,
    
    -- Denormalized from joins
    t.Track_Name,
    b.Branch_Name,
    i.Intake_Name,
    i.Intake_End_Date AS Expected_Graduation_Date,

    -- *** THIS IS THE CORRECTED SECTION ***
    -- We select the column [Facebook] from the CTE 
    -- and ALIAS it as Social_Facebook to match the destination.
    soc.[Facebook] AS Social_Facebook,
    soc.[LinkedIn] AS Social_LinkedIn,
    soc.[GitHub] AS Social_GitHub,
    soc.[X] AS Social_X,
    soc.[Instagram] AS Social_Instagram

FROM 
    Student s
-- Join chain to get Track/Branch/Intake info
LEFT JOIN 
    [Group] g ON s.Intake_Branch_Track_ID = g.Group_ID
LEFT JOIN 
    Track t ON g.Track_ID = t.Track_ID
LEFT JOIN 
    Branch b ON g.Branch_ID = b.Branch_ID
LEFT JOIN 
    Intake i ON g.Intake_ID = i.Intake_ID
-- Join CTE to get the single phone number
LEFT JOIN 
    PhoneCTE p ON s.Student_ID = p.Student_ID AND p.rn = 1
-- Join CTE to get the pivoted social links
LEFT JOIN 
    SocialCTE soc ON s.Student_ID = soc.Student_ID;</property>
