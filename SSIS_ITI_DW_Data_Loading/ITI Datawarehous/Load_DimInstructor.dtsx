                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">/*
This query loads data from the OLTP for the DimInstructor table.
It handles multiple phone numbers and derives the Is_Manger flag.
*/

-- Use a Common Table Expression (CTE) to handle multiple phone numbers.
-- This selects only one phone number per instructor.
WITH PhoneCTE AS (
    SELECT 
        Instructor_ID, 
        Phone,
        -- Assign a row number to each phone, partitioned by instructor
        ROW_NUMBER() OVER(PARTITION BY Instructor_ID ORDER BY Phone) as rn
    FROM 
        Instructor_Phone
)
SELECT 
    i.Instructor_ID,
    i.Instructor_Fname + N' ' + i.Instructor_Lname AS Instructor_FullName,
    i.Instructor_Gender,
    i.Instructor_Birthdate,
    i.Instructor_Marital_Status,
    i.Instructor_Contract_Type,
    i.Instructor_Salary,
    i.Instructor_Email,
    p.Phone, -- Select the single phone from the CTE
    
    -- Check if the instructor is a manager by seeing if their ID
    -- exists in the Manager_ID column of the Department table.
    CASE 
        WHEN EXISTS (
            SELECT 1 
            FROM Department d_mgr 
            WHERE d_mgr.Manager_ID = i.Instructor_ID
        ) 
        THEN N'Yes' 
        ELSE N'No' 
    END AS Is_Manger,
    
    d.Department_Name
FROM 
    Instructor i
-- Join to get the instructor's department name
LEFT JOIN 
    Department d ON i.Department_ID = d.Department_ID
-- Join the CTE to get only the first phone number
LEFT JOIN 
    PhoneCTE p ON i.Instructor_ID = p.Instructor_ID AND p.rn = 1;</property>
